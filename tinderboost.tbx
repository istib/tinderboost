<menu name="Value" kind="stamps" >
<stamp name="Adornment - Comment" attribute="" >$NameFont=Helvetica Neue light; $NameAlignment=left; $Color=transparent; $MapTextSize=50; Width=6; Height=1; NameColor=red</stamp>
<stamp name="Adornment - Header" attribute="" >$NameFont=Helvetica Neue light; $NameAlignment=center; $Color=transparent; $MapTextSize=150; Width=6; Height=3; Border=0</stamp>
<stamp name="Adornment - Preferred Style" attribute="" >NameAlignment=left; NameColor=black; Border=0;</stamp>
<stamp name="Agent - Turn Off" attribute="" >$AgentPriority=-1</stamp>
<stamp name="Agent - Turn On" attribute="" >$AgentPriority=10</stamp>
<stamp name="Case - All Lowercase" attribute="" >$Name = lowercase($Name)</stamp>
<stamp name="Case - All Uppercase" attribute="" >$Name = uppercase($Name)</stamp>
<stamp name="Case - Capitalize First Letter" attribute="" >if (Name((^[a-z])(.+))) { $Name=capitalize($1)+$2 }; Name=Name.replace(&quot;^\s+&quot;,&quot;&quot;)</stamp>
<stamp name="Case - Capitalize Words" attribute="" >$Name = capitalize(lowercase($Name))</stamp>
<stamp name="Check - Toggle" attribute="" >if ($Checked) { $Checked=false} else {$Checked=true}</stamp>
<stamp name="Check Media" attribute="" >Checked=true; EndDate=today; Owned=false; Flagged=false; if (StartDate=never) { StartDate=Modified; EndDate=Modified } else { EndDate=StartDate }</stamp>
<stamp name="Children - Delete Text" attribute="" >$Text(children) = &quot;&quot;</stamp>
<stamp name="Clear Styles" attribute="" >$Prototype=; $Color=; $OutlineBackgroundColor=; $NameBold=false; $DisplayExpression=; $OutlineTextSize=; $MapTextSize=; NameFont=;</stamp>
<stamp name="Color - Set" attribute="" >$Color=`pbpaste</stamp>
<stamp name="Color - Set Map Background Color" attribute="" >$MapBackgroundColor=`pbpaste</stamp>
<stamp name="Color - Set NameColor" attribute="" >$NameColor = `pbpaste</stamp>
<stamp name="Color - Set OutlineBackColor" attribute="" >$OutlineBackgroundColor=`pbpaste</stamp>
<stamp name="DisplayExpression - Clear" attribute="" >$DisplayExpression =;</stamp>
<stamp name="DisplayExpression - Show DescendantCount" attribute="" >$DisplayExpression=&quot;$Name + &apos; (&apos; + $DescendantCount + &apos;)&apos;&quot;</stamp>
<stamp name="Export - No" attribute="" >$HTMLDontExport=true; $KeyAttributes=$KeyAttributes - &quot;HTMLDontExport;HTMLExportChildren&quot;;</stamp>
<stamp name="Export - No + No Children" attribute="" >$HTMLDontExport=true; $HTMLExportChildren=false;</stamp>
<stamp name="Export - Yes" attribute="" >$HTMLDontExport=false;</stamp>
<stamp name="Export - Yes + Children" attribute="" >$HTMLDontExport=false; $HTMLExportChildren=true;</stamp>
<stamp name="Filter Name" attribute="" >Name = escapeHTML(Name).replace(&quot;- &quot;, &quot;&quot;).replace(&quot;&amp;#x2014;&quot;,&quot; -- &quot;).replace(&quot;&amp;#x201c;&quot;,&quot;&apos;&quot;).replace(&quot;&amp;#x201d;&quot;,&quot;&apos;&quot;).replace(&quot;&amp;#x2020;&quot;,&quot;&quot;).replace(&quot;&amp;amp;&quot;,&quot;&amp;&quot;).replace(&quot;&amp;mdash;&quot;,&quot;-&quot;).replace(&quot;\|&quot;,&quot;-&quot;).replace(&quot;&amp;#x2019;&quot;,&quot;&apos;&quot;).replace(&quot;&amp;#x2013;&quot;, &quot;--&quot;)</stamp>
<stamp name="Filter Name - Clear Tags" attribute="" >if (Name((.+) @(\w+))) { $Name=$1 }</stamp>
<stamp name="Filter Name - Remove *" attribute="" >Name=Name.replace(&quot;\*&quot;,&quot;&quot;)</stamp>
<stamp name="Filter Name - Remove Dots" attribute="" >$Name = $Name.replace(&quot;.&quot;,&quot;&quot;)</stamp>
<stamp name="Filter Name - Remove HTML garb" attribute="" >Name=Name.replace(&quot;(&amp;[^ ]+;)&quot;,&quot;&quot;)</stamp>
<stamp name="Filter Name - Remove Skim Hyphens" attribute="" >$Name = $Name.replace(&quot;- &quot;, &quot;&quot;)</stamp>
<stamp name="Filter Name - Remove Square Brackets" attribute="" >Name = Name.replace(&quot;\[.+\]&quot;, &quot;&quot;)</stamp>
<stamp name="Filter Name - Remove character from clipboard" attribute="" >Name=Name.replace(runCommand(&quot;pbpaste&quot;),&quot;&quot;)</stamp>
<stamp name="Filter Name - Remove numeration" attribute="" >if (Name((^.{1,3}\.|\) *)(.+))) { Name=$2 }</stamp>
<stamp name="Filter Name - Reverse with Hyphen" attribute="" >if (Name((.+) - (.+))) { Name = $2 +&quot; - &quot;+$1}</stamp>
<stamp name="Filter Text" attribute="" >Text = escapeHTML(Text).replace(&quot;- &quot;, &quot;&quot;).replace(&quot;&amp;#x2014;&quot;,&quot; -- &quot;).replace(&quot;&amp;#x201c;&quot;,&quot;&apos;&quot;).replace(&quot;&amp;#x201d;&quot;,&quot;&apos;&quot;).replace(&quot;&amp;#x2020;&quot;,&quot;&quot;).replace(&quot;&amp;amp;&quot;,&quot;&amp;&quot;).replace(&quot;&amp;mdash;&quot;,&quot;-&quot;).replace(&quot;\|&quot;,&quot;-&quot;).replace(&quot;&amp;#x2019;&quot;,&quot;&apos;&quot;)</stamp>
<stamp name="Filter Text - Clear Tags" attribute="" >if (Text((.+) @(\w+))) { $Text=$1 }</stamp>
<stamp name="Filter Text - Remoke Square Brackets" attribute="" >Text = Text.replace(&quot;\[.+\]&quot;, &quot;&quot;)</stamp>
<stamp name="Filter Text - Remove HTML garb" attribute="" >Text=Text.replace(&quot;(&amp;[^ ]+;)&quot;,&quot;&quot;)</stamp>
<stamp name="Filter Text - Remove Skim Codes" attribute="" >if (Text((^.+ )(@\w+$))) { Text=$1 }</stamp>
<stamp name="Filter Text - Remove Skim Hyphens" attribute="" >$Text = $Text.replace(&quot;- &quot;, &quot;&quot;)</stamp>
<stamp name="Filter Text - Remove character from clipboard" attribute="" >Text=Text.replace(runCommand(&quot;pbpaste&quot;),&quot;&quot;)</stamp>
<stamp name="Filter Text - Replace numerators" attribute="" >Text=Text.replace(&quot;(\d+\. )&quot;,&quot;&quot;)</stamp>
<stamp name="Filter Text - Strip quotes" attribute="" >`echo &quot;$Text&quot; | sed -e &quot;s/[‘’]/&apos;/g&quot; | sed -e &apos;s/[“”?]/&quot;/g&apos; | pbcopy</stamp>
<stamp name="Flagged - Toggle" attribute="" >if ($Flagged) { $Flagged=false; $NameBold=false;} else {$Flagged=true; $NameBold=true}</stamp>
<stamp name="Font - Decrease Size" attribute="" >$MapTextSize = $MapTextSize - 5; $OutlineTextSize = $OutlineTextSize - 5</stamp>
<stamp name="Font - Increase Size" attribute="" >$MapTextSize = $MapTextSize + 5; $OutlineTextSize = $OutlineTextSize + 5</stamp>
<stamp name="Get Value from Clipboard" attribute="" >runCommand(&quot;echo &quot;+eval(`pbpaste)+&quot;| pbcopy&quot;)</stamp>
<stamp name="Implode Children" attribute="" >if ($Text) { $Text = $Text + &quot;\n&quot; + format(collect(child, $Name),&quot;\n&quot;) } else { $Text = format(collect(child, $Name),&quot;\n&quot;) }</stamp>
<stamp name="Implode Children and Trash" attribute="" >if ($Text) { $Text = $Text + &quot;\n&quot; + format(collect(child, $Name),&quot;\n&quot;) } else { $Text = format(collect(child, $Name),&quot;\n&quot;) }; Container(children)=&quot;/Gears/Trash&quot;</stamp>
<stamp name="Implode Children in Bullet Points" attribute="" >$Text = &quot;- &quot;+format(collect(child, $Name),&quot;\n- &quot;) }</stamp>
<stamp name="Implode Children&apos;s TITLE and Text" attribute="" >Text=^include(this, _TitleCapTextChildren)</stamp>
<stamp name="Implode Children&apos;s Text" attribute="" >$Text = $Text +&quot;\n\n&quot;+format(collect(child, $Text),&quot;\n&quot;)</stamp>
<stamp name="Implode Children&apos;s Title and Text" attribute="" >Text=^include(this, _TitleTextChildren)</stamp>
<stamp name="Implode Descendents" attribute="" >if ($Text) { $Text = $Text + &quot;\n&quot; + format(collect(descendent, $Name),&quot;\n&quot;) } else { $Text = format(collect(descendent, $Name),&quot;\n&quot;) }</stamp>
<stamp name="Implode Descendents in Bullet Points" attribute="" >Text=^include(this, _BulletPointList)</stamp>
<stamp name="KeyAttributes - Add" attribute="" >$KeyAttributes=$KeyAttributes + `pbpaste</stamp>
<stamp name="KeyAttributes - Clear" attribute="" >$KeyAttributes=;</stamp>
<stamp name="KeyAttributes - None" attribute="" >$KeyAttributes=&quot;&quot;</stamp>
<stamp name="Link to Clipboard" attribute="" >linkTo(`pbpaste)</stamp>
<stamp name="Map - Hide Children" attribute="" >$TableExpression=; $TitleHeight =; $MapTextSize=;</stamp>
<stamp name="Map - Hide Text" attribute="" >MapBodyTextSize=1</stamp>
<stamp name="Map - Horizontal Line" attribute="" >Height=0; Width=20; Prototype=*Adornment</stamp>
<stamp name="Map - Move down" attribute="" >$Ypos = $Ypos + 1.5</stamp>
<stamp name="Map - Move left" attribute="" >$Xpos = $Xpos - 1.5</stamp>
<stamp name="Map - Move right" attribute="" >$Xpos = $Xpos + 1.5</stamp>
<stamp name="Map - Move up" attribute="" >$Ypos = $Ypos - 1.5</stamp>
<stamp name="Map - Title Only" attribute="" >$TitleHeight = $Height</stamp>
<stamp name="Move Children to Trash" attribute="" >Container(children)=&apos;/Gears/Trash&apos;</stamp>
<stamp name="Move to Clipboard" attribute="" >$Container=`pbpaste</stamp>
<stamp name="Move to Inbox" attribute="" >$Container=&quot;/Inbox&quot;</stamp>
<stamp name="Move to Trash" attribute="" >Container=&quot;/Gears/Trash&quot;</stamp>
<stamp name="Name - Append Next" attribute="" >Name=Name+&quot; &quot;+$Name(next); Container(next)=&quot;/Gears/Trash&quot;</stamp>
<stamp name="Name - Append Next (Parenthesis)" attribute="" >Name=Name+&quot; (&quot;+$Name(next) + &apos;)&apos;; Container(next)=&quot;/Gears/Trash&quot;</stamp>
<stamp name="Name - Append Next (Semicolon)" attribute="" >Name=Name+&quot;: &quot;+$Name(next); Container(next)=&quot;/Gears/Trash&quot;</stamp>
<stamp name="Name - Copy" attribute="" >runCommand(&quot;printf &apos;&quot; + $Name +&quot;&apos; | pbcopy&quot;)</stamp>
<stamp name="Name - Paste" attribute="" >$Name=`pbpaste</stamp>
<stamp name="Name - Text into Name (Return)" attribute="" >$Name = $Name + &apos;\n &apos; + $Text; $Text=;</stamp>
<stamp name="Name - Text into Name (Semicolon)" attribute="" >$Name = $Name + &apos;: &apos; + $Text; $Text=;</stamp>
<stamp name="New Article" attribute="" >$Prototype=&quot;*Article&quot;; $EndDate=today; $Checked=true;</stamp>
<stamp name="OnAdd - Blank" attribute="" >OnAdd=&quot;&quot;;</stamp>
<stamp name="OnAdd - Clear" attribute="" >OnAdd=;</stamp>
<stamp name="OnAdd - Skim Format" attribute="" >OnAdd=OnAdd(*Skim Import)</stamp>
<stamp name="Opacity - 0" attribute="" >Opacity=0</stamp>
<stamp name="Opacity - 100" attribute="" >Opacity=100</stamp>
<stamp name="Opacity - 40" attribute="" >Opacity=40</stamp>
<stamp name="Owned" attribute="" >$Owned=true</stamp>
<stamp name="Owned as PDF" attribute="" >Prototype=&quot;*Book&quot;; Owned=true; $Kind=&quot;PDF&quot;</stamp>
<stamp name="Prototype - Clear" attribute="" >$Prototype=; $Badge=;</stamp>
<stamp name="Prototype - Create" attribute="" >Name=&quot;*&quot;+Name; IsPrototype=true</stamp>
<stamp name="Prototype - Preserve" attribute="" >$xPrototype=$Prototype;</stamp>
<stamp name="Prototype - Restore" attribute="" >$Prototype=$xPrototype; $xPrototype=;</stamp>
<stamp name="Prototype - Set" attribute="" >$Prototype=&apos;*&apos;+`pbpaste; KeyAttributes=; $Badge=;</stamp>
<stamp name="Prototype - Set Same as Previous" attribute="" >$Prototype=$Prototype(prevSibling); $Badge=;</stamp>
<stamp name="Question - Make Question" attribute="" >if (!Name((\?$))) { Name=Name+&quot;?&quot; }; if (!Prototype) { Prototype=&quot;*Question&quot; }</stamp>
<stamp name="Rule - Clear" attribute="" >$Rule=;</stamp>
<stamp name="Rule - Copy" attribute="" >runCommand(&quot;printf &apos;&quot;+Rule +&quot;&apos;  | pbcopy&quot;)</stamp>
<stamp name="Rule - Paste" attribute="" >$Rule=`pbpaste</stamp>
<stamp name="Separator - Toggle" attribute="" >if (Separator) { Separator=false } else { Separator=true }</stamp>
<stamp name="Separator Line" attribute="" >$Name=; Color=red; Separator=true; Prototype=&apos;&apos;;</stamp>
<stamp name="Shadow - Clear" attribute="" >Shadow=;</stamp>
<stamp name="Shadow - None" attribute="" >Shadow=0</stamp>
<stamp name="Size - Decrease Height" attribute="" >Height = Height -1</stamp>
<stamp name="Size - Decrease Width" attribute="" >Width = Width-1</stamp>
<stamp name="Size - Increase Height" attribute="" >Height = Height+1</stamp>
<stamp name="Size - Increase Width" attribute="" >Width = Width+1</stamp>
<stamp name="Size - Large" attribute="" >Height=4; Width=8</stamp>
<stamp name="Size - Long" attribute="" >Height=1.5; Width=4</stamp>
<stamp name="Size - Medium" attribute="" >Height=2.5; Width=5</stamp>
<stamp name="Size - Small" attribute="" >Height=1; Width=3</stamp>
<stamp name="Sort - Don&apos;t Sort" attribute="" >Sort=&apos;&apos;; SortAlso=&apos;&apos;;</stamp>
<stamp name="Source - Credit Parent Note" attribute="" >KeyAttributes=KeyAttributes+&quot;Source&quot;; Source = Name(parent);</stamp>
<stamp name="Source - Credit in Name" attribute="" >DisplayExpression = &apos;Name+&quot; (&quot;+Source+&quot;)&quot;&apos;</stamp>
<stamp name="Source - Paste" attribute="" >Source=`pbpaste</stamp>
<stamp name="Status - Negative" attribute="" >Status=&quot;negative&quot;</stamp>
<stamp name="Status - Positive" attribute="" >Status=&quot;positive&quot;</stamp>
<stamp name="Swatch - Hide" attribute="" >OutlineColorSwatch = false</stamp>
<stamp name="Swatch - Show" attribute="" >OutlineColorSwatch = true</stamp>
<stamp name="Tags - Append" attribute="" >Tags=Tags+`pbpaste; if (!KeyAttributes.contains(&quot;Tags&quot;)) { KeyAttributes=KeyAttributes+&quot;Tags&quot;}</stamp>
<stamp name="Tags - Display in Title" attribute="" >if (!DisplayExpression) { DisplayExpression=&quot;Name+&apos; | &apos;+format(Tags,&apos; &apos;)&quot; } else { DisplayExpression=; }</stamp>
<stamp name="Tags - Extract from Title" attribute="" >if(Name((.+) \| (.+))){Name=$1; Tags=Tags+$2.replace(&quot; &quot;,&quot;;&quot;)}; if (!KeyAttributes.contains(&quot;Tags&quot;)) { KeyAttributes=KeyAttributes+&quot;Tags&quot;}</stamp>
<stamp name="Tags - Set Prototype" attribute="" >if(Tags.contains(&quot;\*(.+)&quot;)){Prototype=&quot;*&quot;+capitalize($1)}</stamp>
<stamp name="Text - Append Paste" attribute="" >$Text=$Text + &quot;\n&quot; +`pbpaste</stamp>
<stamp name="Text - Append next" attribute="" >$Text=$Text+&quot; &quot;+$Text(next); Container(next)=/Gears/Trash</stamp>
<stamp name="Text - Copy" attribute="" >runCommand(&quot;export __CF_USER_TEXT_ENCODING=0x1F5:0x8000100:0x8000100&quot;); runCommand(&quot;echo &apos;&quot; + $Text +&quot;&apos; | pbcopy&quot;)</stamp>
<stamp name="Text - Delete" attribute="" >$Text = &quot;&quot;</stamp>
<stamp name="Text - Name + Text" attribute="" >$Text = $Name + &apos;\n&apos; + $Text</stamp>
<stamp name="Text - Paste" attribute="" >$Text=`pbpaste; FilterText</stamp>
<stamp name="Title - Toggle" attribute="" >if (ShowTitle) { ShowTitle=false } else { ShowTitle=true }</stamp>
<stamp name="URL - Copy" attribute="" >runCommand(&quot;echo &apos;&quot;+$URL +&quot;&apos;  | pbcopy&quot;)</stamp>
<stamp name="URL - Create" attribute="" >$Prototype=*Link; $URL=`pbpaste</stamp>
<stamp name="URL - Import from Child Name" attribute="" >$URL = $Name(child); Prototype=*Link</stamp>
<stamp name="URL - Import from Name" attribute="" >$Prototype=*Link; $URL=$Name;</stamp>
<stamp name="URL - Import from NameAtLink" attribute="" >$Prototype=*Link; if (Name((.+) @ (.+))) { $Name=$1; $URL=$2; }</stamp>
<stamp name="URL - InstaRead" attribute="" >runCommand(&quot;echo http://www.instapaper.com/text?u=&quot;+urlEncode(URL)+&quot; | pbcopy&quot;)</stamp>
<stamp name="URL - Paste" attribute="" >$URL=`pbpaste</stamp>
</menu>
